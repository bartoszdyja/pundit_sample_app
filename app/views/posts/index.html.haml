%h1 All posts
%table
  %tr
    %th Post title
    %th User
    %th User admin?
    %th Action
  -@posts.each do |post|
    %tr
      %td=post.title
      %td=post.user.name
      %td=post.user.admin
      %td=link_to 'Destroy', post_path(post), method: :delete
%hr
=link_to 'Add new_post', new_post_path
